browser "librewolf %u"

macro v set browser "yt-dlp -f 'bv*[height=720]+ba' %u" ; open-in-browser ; set browser "librewolf %u" 


# unbind keys
unbind-key ENTER
unbind-key j
unbind-key k
unbind-key J
unbind-key K

# bind keys - vim style
bind-key j down
bind-key k up
bind-key l open
bind-key h quit
