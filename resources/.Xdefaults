XTerm.vt100.foreground: white
XTerm.vt100.background: black

XTerm.vt100.faceName: Noto Mono:antialias=true
XTerm.vt100.faceSize: 12

XTerm.termName: xterm-256color
XTerm.vt100.locale: false
XTerm.vt100.utf8: true

XTerm.vt100.bellIsUrgent: false

XTerm.vt100.translations: #override \n\
Ctrl <Key> minus: smaller-vt-font() \n\
Ctrl <Key> plus: larger-vt-font() \n\
Ctrl <Key>M: maximize() \n\
Ctrl <Key>R: restore() \n\
Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
Ctrl Shift <Key>P: insert-selection(CLIPBOARD) \n\
Ctrl Alt <Key>S: secure()

XTerm.vt100.metaSendsEscape: true

XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?

XTerm.vt100.saveLines: 4096

<Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)


